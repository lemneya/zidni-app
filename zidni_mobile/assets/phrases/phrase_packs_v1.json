        {
          "id": "taxi_10",
          "arabic": "شكراً، احتفظ بالباقي",
          "translations": {
            "zh": "谢谢，不用找了",
            "en": "Thank you, keep the change",
            "tr": "Teşekkürler, üstü kalsın",
            "es": "Gracias, quédese con el cambio"
          }
        }
      ]
    },
